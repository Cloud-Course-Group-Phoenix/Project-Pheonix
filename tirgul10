        "spark = SparkSession.builder.appName(\"RDD Example\").getOrCreate()\n",
        "\n",
        "rdd = spark.sparkContext.parallelize([(\"apple\", 1), (\"banana\", 1), (\"apple\", 1)])\n",
        "result = rdd.reduceByKey(lambda a, b: a + b)\n",
        "print(result.collect())\n"
